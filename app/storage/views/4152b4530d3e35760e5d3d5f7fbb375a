<p><?php echo link_to_route('sinh_viens.create', 'Add new sinh_vien'); ?></p>

<?php if ($sinh_viens->count()): ?>
	<table class="table table-striped table-bordered">
		<thead>
			<tr>
				<th>Mssv</th>
				<th>Ho_ten</th>
				<th>Gioi_tinh</th>
				<th>Ngay_sinh</th>
				<th>Dia_chi</th>
				<th>Que_quan</th>
				<th>Email</th>
				<th>Sdt</th>
				<th>Ghi_chu</th>
				<th>Lops_Id</th>
				<th>TaiKhoans_Id</th>
			</tr>
		</thead>

		<tbody>
			<?php foreach ($sinh_viens as $sinh_vien): ?>
				<tr>
					<td><?php echo e($sinh_vien->mssv); ?></td>
					<td><?php echo e($sinh_vien->ho_ten); ?></td>
					<td><?php echo e($sinh_vien->gioi_tinh); ?></td>
					<td><?php echo e($sinh_vien->ngay_sinh); ?></td>
					<td><?php echo e($sinh_vien->dia_chi); ?></td>
					<td><?php echo e($sinh_vien->que_quan); ?></td>
					<td><?php echo e($sinh_vien->email); ?></td>
					<td><?php echo e($sinh_vien->sdt); ?></td>
					<td><?php echo e($sinh_vien->ghi_chu); ?></td>
					<td><?php echo e($sinh_vien->Lops_Id); ?></td>
					<td><?php echo e($sinh_vien->TaiKhoans_Id); ?></td>
                    <td><?php echo link_to_route('sinh_viens.edit', 'Edit', array($sinh_vien->id), array('class' => 'btn btn-info')); ?></td>
                    <td>
                        <?php echo Form::open(array('method' => 'DELETE', 'route' => array('sinh_viens.destroy', $sinh_vien->id))); ?>
                            <?php echo Form::submit('Delete', array('class' => 'btn btn-danger')); ?>
                        <?php echo Form::close(); ?>
                    </td>
				</tr>
			<?php endforeach; ?>
		</tbody>
	</table>
<?php else: ?>
	There are no sinh_viens
<?php endif; ?>

