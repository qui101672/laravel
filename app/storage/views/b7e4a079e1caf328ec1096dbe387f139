                    <td><?php echo link_to_route('thanh_vien_bgks.edit', 'Edit', array($thanh_vien_bgk->id), array('class' => 'btn btn-info')); ?></td>
                    <td>
                        <?php echo Form::open(array('method' => 'DELETE', 'route' => array('thanh_vien_bgks.destroy', $thanh_vien_bgk->id))); ?>
                            <?php echo Form::submit('Delete', array('class' => 'btn btn-danger')); ?>
                        <?php echo Form::close(); ?>
                    </td>
				</tr>
			<?php endforeach; ?>
