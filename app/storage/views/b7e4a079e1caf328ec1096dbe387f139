			</tr>
		</thead>

		<tbody>
			<?php foreach ($thanh_vien_bgks as $thanh_vien_bgk): ?>
				<tr>
					<td><?php echo e($thanh_vien_bgk->ma_thanh_vien); ?></td>
					<td><?php echo e($thanh_vien_bgk->ho_ten); ?></td>
